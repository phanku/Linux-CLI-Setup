filetype indent on
filetype plugin on
syntax enable
set nobackup
set ruler
set title
set incsearch
set ignorecase
set smartcase
set cindent
set smartindent
set autoindent
set scrolloff=2
set wildmode=longest,list
set tabstop=3
set shiftwidth=3
set expandtab
set fileencodings=utf-8
:hi Comment ctermfg=yellow
hi Directory ctermfg=yellow
highlight! link DiffText MatchParen
